s_username="emc"
s_password="123"

uname=input("enter the name of the user:")
password=input("enter the password:")

def validate():
    if(s_username==uname and s_password==password):
        return True
    else:
        return False
a=validate()
print(a)

def add(n1,n2):
    return n1+n2
a=int(input("enter the values of a:"))
b=int(input("enter the vaues of b:"))
c=int(input("enter the values of c:"))
add=add(a,b)
output=add*c
print(output)
